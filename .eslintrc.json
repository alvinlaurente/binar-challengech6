{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node" : true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "global-require": "off",
        "import/no-dynamic-require": "off",
        "no-console": "off",
        "no-plusplus": "off",
        "no-unused-vars": "off"
    }
}
